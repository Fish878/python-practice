public class Main
    
 {

    public static void main(String[] args) {
        String input = "Seal eat Fish";

        int key=3;

        char[] chars = input.toCharArray();
        StringBuilder ZN = new StringBuilder();
        for (char aChar : chars) {
            int b=aChar;

            b=b+3;
            char newb=(char)b;
            ZN.append(newb);
        }
        System.out.println("密文==="+ZN.toString());
    }
}
